def asteroidCollision(asteroid):
    st=[]
    for ast in asteroid:
        while st and ast<0 and st[-1]>0:
            if st[-1]<-ast:
                st.pop()
                continue
            elif st[-1]==-ast:
                st.pop()
            break
        else:
            st.append(ast)
    return st 
asteroid=[5,10,-5]
print(asteroidCollision(asteroid))           
