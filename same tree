class Solution:
    def isSameTree(self,p,q):  
        if not p and  not q:
            return True
        if not p or not q:
            return False
        if p.val!=q.val:
            return False
        left=self.isSameTree(p.left,p.left)
        right=self.isSameTree(p.right,q.right)
        if left and right:
            return True
        else:
            return False
