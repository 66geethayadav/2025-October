def nextGreaterElement(nums1, nums2):
        st=[]
        nge={}
        for i in reversed(nums2):
            while st and st[-1]<=i:
                st.pop()
            if st:
                nge[i]=st[-1]
            else:
                nge[i]=-1
            st.append(i) 
        res=[]
        for ch in nums1:
            res.append(nge[ch])
nums1=[4,1,2]
nums2=[1,2,3,4] 
print(nextGreaterElement(nums1,nums2))           
